<div class="row">
<div class="medium-8 columns">
  <% @stories.each_with_index do |story, index| %>
  <div class="story">
    <div class="description">
      <%= story.description %>
    </div>
    <div>
      <div class="row collapse">
        <div class="medium-3 columns">
          <%= link_to 'I agree, your life rocks!', "#!", id: "agree-button-#{index}", onclick: "confirmLml('agree-button-#{index}')" %>
        </div>
        <div class="medium-3 columns left">
          <%= link_to 'I\'ve seen better.', "#!", id: "disagree-button-#{index}", onclick: "disagreeLml('disagree-button-#{index}')" %>
        </div>
      </div>
    </div>
  </div>
  <% end %>
</div>
</div>
<div class="row">
  <div class="small-12 columns">
    <div class="story-pagination">
      <%= will_paginate @tutorials, previous_label: "Previous", next_label: "Next" %>
    </div>
  </div>
</div>
