<div class="row">
<div class="medium-8 columns">
  <% @stories.each_with_index do |story, index| %>
  <div class="story">
    <div class="description">
      <%= story.description %>
    </div>
    <div>
      <%= link_to 'I agree, your life rocks', "#!", id: "agree-button-#{index}", onclick: "confirmFml('agree-button-#{index}')" %>
    </div>
  </div>
  <% end %>
</div>
</div>
<div class="row">
  <div class="small-12 columns">
    <div class="story-pagination">
      <%= will_paginate @tutorials, previous_label: "Previous", next_label: "Next" %>
    </div>
  </div>
</div>
